{
    "name": "Screen Card",
    "description": "  ",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_icon": "/img/sc.png"

    },

    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["injection.js"]
    }],
    "permissions": [
        "tabs", "activeTab"
    ],
    "web_accessible_resources": [{
        "resources": ["/assets/*", "/img/", "iconfont"],
        "matches": ["<all_urls>"]
    }]

}