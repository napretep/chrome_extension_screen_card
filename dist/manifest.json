{
    "name": "screen card2",
    "description": "  ",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16":"fable.png",
            "32":"fable.png"
        }
    },

    "content_scripts": [
        {
            "matches": ["http://*/*","https://*/*"],
            "js": ["injection.js"]
        }
    ],
    "permissions": [
        "tabs",  "activeTab"
    ],
    "web_accessible_resources": [{
  "resources": ["/assets/*"],
  "matches": ["<all_urls>"]
}]

}